<h1 class="index-title">Shipping addresses</h1>

<table class = "table">
  <thead class="thead-dark">
    <tr>
      <th scope="col">Street number</th>
      <th scope="col">City</th>
      <th scope="col">State</th>
      <th scope="col">Zip code</th>
      <th scope="col">Country</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    <% @shipping_addresses.each do |shipping_address| %>
      <tr>
        <td><%= shipping_address.street_number %></td>
        <td><%= shipping_address.city %></td>
        <td><%= shipping_address.state %></td>
        <td><%= shipping_address.zip_code %></td>
        <td><%= shipping_address.country %></td>
      </tr>
    <% end %>
  </tbody>
</table>